{
  "name": "wavesys-website",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "npm run watch:css & BROWSER=none react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/styles/style.css -o src/index.css",
    "watch:css": "postcss -w src/styles/style.css -o src/index.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.8.4",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.4.6"
  }
}